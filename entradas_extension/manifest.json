{
    "manifest_version": 3,
    "name": "Entradas Bot",
    "version": "1.0",
    "description": "Entradas Bot Configuration",
    "content_scripts": [
      {
        "matches": ["*://*.entradas.com/*", "https://www.google.com/"],
        "js": ["src/content_scripts/content.js"],
        "all_frames": true
      }

    ],
    "web_accessible_resources": [
      {
        "resources": ["src/images/stadium.png", "html/htmlFileName.html", "styles/*", "fonts/*", "src/content_scripts/iframe_inject.js"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
    "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "declarativeNetRequest",
    "scripting",
    "https://www.google.com/recaptcha/",
    "https://www.google.com/"

  ],
      "default_popup": "src/popup/popup.html"
    },
    "host_permissions": [
    "*://*/*"
  ]
  }
{
    "manifest_version": 3,
    "name": "AI Captcha Solver",
    "version": "1.0",
    "description": "AI Captcha Solver",
    "background": {
    "service_worker": "dist/bundle.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content_scripts/content.js"]
      }
    ],
    "action": {
    "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "declarativeNetRequest",
    "scripting"
  ],
      "default_popup": "src/popup/popup.html"
    },
    "host_permissions": [
    "*://*/*"
  ]
  }